{
  "name": "bs-knex",
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "bs-dependencies": ["bs-node-debug"],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "reason": {
    "react-jsx": 2
  },
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "refmt": 3
}
